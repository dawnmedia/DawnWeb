---
// Import the necessary components
import MainLayout from "@/layouts/MainLayout.astro";
import ContactSection from "@components/sections/fr/ContactSection_fr.astro";
---

<!--Utilizing MainLayout for the outer layout of the page, and defining meta for SEO purposes-->
<MainLayout
  title="Contact | Dawn Web"
  lang="br"
  structuredData={{
    "@context": "https://schema.org",
    "@type": "WebPage",
    "@id": "https://dawnwebs.com/br/contact",
    url: "https://dawnwebs.com/br/contact",
    name: "Nous Contacter | Dawn Web",
    description:
      "Des questions ou un projet à discuter ? Contactez-nous et créons ensemble la solution idéale grâce à nos services.",
    isPartOf: {
      "@type": "WebSite",
      url: "https://dawnwebs.com/br/",
      name: "Dawn Web",
      description:
        "Dawn Web crée des sites web remarquables et propose des services experts en marketing en ligne pour aider les entreprises à prospérer. Découvrez nos services et contactez-nous pour booster votre présence en ligne.",
    },
    inLanguage: "br",
  }}
>
  <ContactSection />
</MainLayout>
